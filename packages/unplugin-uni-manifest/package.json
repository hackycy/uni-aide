  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@uni-aide/core": "workspace:^",
    "@uni-aide/types": "workspace:^",
    "unplugin": "catalog:",
    "unplugin-utils": "catalog:"
  },
  "devDependencies": {
    "vite": "catalog:"
  }
}
